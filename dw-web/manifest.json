{
   "name": "dw-web",
   "version": "0.1",
   "description": "aplicaci√≥n para descargar paginas web",
   "manifest_version": 2,
   "browser_action": {
      "default_popup": "popup.html"
    },
   "content_scripts":[ {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
   }],
   "background":{
     "scripts": [
       "background.js",
       "./modules/transform_dom.js"
      ]
   },
   "permissions":["tabs","<all_urls>"]
 }
