{
   "name": "dw-web",
   "version": "0.1",
   "description": "aplicaci√≥n para descargar paginas web",
   "manifest_version": 2,
   "browser_action": {
      "default_popup": "popup.html"
    },
   "content_scripts":[ {
    "matches": ["<all_urls>"],
    "js":
    [
      "content.js",
      "modules/Utiles.js",
      "modules/jspdf.js"
    ]
   }],
   "background":{
     "scripts": [
       "background.js"
      ]
   },
   "permissions":["tabs","<all_urls>"]
 }
