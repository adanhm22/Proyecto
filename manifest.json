{
   "name": "dw-web",
   "version": "0.9.2",
   "description": "aplicaci√≥n para descargar paginas web",
   "manifest_version": 2,
   "browser_action": {
      "default_popup": "popup/popup.html"
    },
    "icons": {
      "16": "res/dwWeb16x16.png",
      "32": "res/dwWeb32x32.png",
      "48": "res/dwWeb48x48.png"
    },
   "content_scripts":[ {
    "matches": ["<all_urls>"],
    "js":
    [
      
      "libraries/html2pdf.bundle.js",
      "libraries/html2canvas.js",
      "libraries/jszip.min.js",
      "libraries/jquery-3.4.1.min.js",

      "content/controlador.js",
      "content/images.js",
      "content/pdf.js",
      "content/htmljs.js",
      "content/galery.js"
    ]
   }],
   "permissions":["<all_urls>","tabs","storage"],
   "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
   
 }
