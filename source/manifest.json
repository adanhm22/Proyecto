{
   "name": "dw-web",
   "version": "0.9",
   "description": "aplicaci√≥n para descargar paginas web",
   "manifest_version": 2,
   "browser_action": {
      "default_popup": "view/popup.html"
    },
    "icons": {
      "16": "res/dwWeb16x16.png",
      "32": "res/dwWeb32x32.png",
      "48": "res/dwWeb48x48.png"
    },
   "content_scripts":[ {
    "matches": ["<all_urls>"],
    "js":
    [
      "controlador.js",
      "modules/html2pdf.bundle.js",
      "modules/html2canvas.js",
      "modules/images.js",
      "modules/pdf.js",
      "modules/htmljs.js",
      "modules/jszip.min.js",
      "modules/jquery-3.4.1.min.js",
      "modules/galery.js",
      "view/script.js"
    ]
   }],
   "permissions":["<all_urls>","tabs","storage"],
   "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
   
 }
